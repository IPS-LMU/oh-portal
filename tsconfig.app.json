{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": [],
    "paths": {
      "@octra/annotation": [
        "../octra/dist/libs/annotation"
      ],
      "@octra/utilities": [
        "../octra/dist/libs/utilities"
      ],
      "@octra/media": [
        "../octra/dist/libs/media"
      ]
    }
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ],
  "exclude": [
    "src/test.ts",
    "src/**/*.spec.ts"
  ]
}
